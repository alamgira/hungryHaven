    <icon height="60" src="res/Tab-bar-icon.png" width="60" />
    <icon height="120" src="res/icon-60@2x.png" width="120" />
    <icon height="76" src="res/Icon-76.png" width="76" />
    <icon height="152" src="res/icon-76@2x.png" width="152" />
    <icon height="57" src="res/Icon.png" width="57" />
    <icon height="114" src="res/Icon@2x.png" width="114" />
    <icon height="72" src="res/Icon-72.png" width="72" />
    <icon height="29" src="res/Icon-small.png" width="29" />
    <icon height="58" src="res/Icon-small@2x.png" width="58" />
